{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "useDefineForClassFields": true,
    "isolatedModules": true,
    "outDir": "dist/",
    "plugins": [{
      "name": "typescript-tslint-plugin",
      "suppressWhileTypeErrorsPresent": true
    }]
  }
}