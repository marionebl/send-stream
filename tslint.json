{
  "defaultSeverity": "error",
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "dist/**"
    ]
  },
  "extends": [
    "tslint-eslint-rules",
    "tslint-microsoft-contrib",
    "tslint-consistent-codestyle/rules",
    "tslint-sonarts"
  ],
  "rules": {
    "adjacent-overload-signatures": true,
    "member-access": [true, "no-public"],
    "member-ordering": [
      true,
      {
        "order": "statics-first"
      }
    ],
    "no-any": true,
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-namespace": true,
    "no-reference": true,
    "no-var-requires": true,
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ],
    "typedef": true,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "ban": [
      true,
      "eval",
      { "name": "$", "message": "please don't" },
      { "name": ["*", "forEach"], "message": "Use a regular for loop instead." }
    ],
    "curly": true,
    "forin": true,
    "label-position": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "log",
      "debug",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-empty": true,
    "no-eval": true,
    "no-invalid-this": true,
    "no-null-keyword": false,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "radix": true,
    "switch-default": false,
    "triple-equals": true,
    "use-isnan": true,
    "eofline": true,
    "indent": [
      true,
      "tabs"
    ],
    "linebreak-style": false,
    "max-file-line-count": false,
    "max-line-length": [
      true,
      120
    ],
    "no-default-export": true,
    "no-mergeable-namespace": true,
    "no-require-imports": true,
    "no-trailing-whitespace": true,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "align": [
      true,
      "arguments",
      "elements",
      "members",
      "parameters",
      "statements"
    ],
    "arrow-parens": false,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "file-header": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsdoc-format": [true, "check-multiline-start"],
    "new-parens": true,
    "no-angle-bracket-type-assertion": false,
    "no-consecutive-blank-lines": true,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-shorthand": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-finally",
      "check-open-brace",
      "check-whitespace"
    ],
    "one-variable-per-declaration": [
      true,
      "ignore-for-loop"
    ],
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "array-type": [true, "array"],
    "prefer-for-of": true,
    "cyclomatic-complexity": false,
    "max-classes-per-file": [true, 5],
    "completed-docs": false,
    "prefer-const": true,
    "strict-boolean-expressions": [
      true,
      "allow-null-union",
      "allow-undefined-union",
      "allow-string",
      "allow-number",
      "allow-mix"
    ],
    "no-magic-numbers": false,
    "import-blacklist": false,
    "promise-function-async": true,
    "no-inferred-empty-object-type": true,
    "no-string-throw": true,
    "no-empty-interface": true,
    "interface-over-type-literal": true,
    "callable-types": true,
    "no-void-expression": true,
    "space-before-function-paren": [true,
      {
        "anonymous": "never",
        "asyncArrow": "always",
        "constructor": "never",
        "method": "never",
        "named": "never"
      }
    ],
    "import-spacing": true,
    "unified-signatures": true,
    "arrow-return-shorthand": true,
    "no-unbound-method": [true, "ignore-static"],
    "no-boolean-literal-compare": true,
    "no-unsafe-any": true,
    "no-unnecessary-qualifier": true,
    "no-unnecessary-initializer": true,
    "await-promise": true,
    "no-floating-promises": true,
    "strict-type-predicates": true,
    "no-misused-new": true,
    "prefer-method-signature": true,
    "prefer-function-over-method": false,
    "no-import-side-effect": true,
    "match-default-export-name": false,
    "no-non-null-assertion": true,
    "ban-types": [true, 
      ["Object", "Avoid using the `Object` type. Did you mean `object`?"],
      ["Function", "Avoid using the `Function` type. Prefer a specific function type, like `() => void`."],
      ["Boolean", "Avoid using the `Boolean` type. Did you mean `boolean`?"],
      ["Number", "Avoid using the `Number` type. Did you mean `number`?"],
      ["String", "Avoid using the `String` type. Did you mean `string`?"],
      ["Symbol", "Avoid using the `Symbol` type. Did you mean `symbol`?"]
    ],
    "no-duplicate-super": true,
    "newline-before-return": false,
    "no-unused-variable": false,
    "prefer-template": true,
    "return-undefined": true,
    "no-reference-import": true,
    "no-unnecessary-callback-wrapper": true,
    "no-invalid-template-strings": true,
    "no-sparse-arrays": true,
    "no-object-literal-type-assertion": true,
    "no-irregular-whitespace": true,
    "prefer-switch": true,
    "number-literal-format": true,
    "deprecation": true,
    "no-unnecessary-type-assertion": true,
    "no-for-in-array": true,
    "restrict-plus-operands": true,
    "no-parameter-properties": false,
    "prefer-object-spread": true,
    "encoding": true,
    "prefer-conditional-expression": true,
    "switch-final-break": [true, "always"],
    "use-default-type-parameter": true,
    "binary-expression-operand-order": true,
    "type-literal-delimiter": true,
    "no-this-assignment": true,
    "no-duplicate-imports": true,
    "no-submodule-imports": false,
    "space-within-parens": [true, 0],
    "no-parameter-reassignment": true,
    "no-return-await": true,
    "no-redundant-jsdoc": true,
    "no-duplicate-switch-case": true,
    "no-implicit-dependencies": [true, "dev"],
    "no-unnecessary-class": false,
    "ban-comma-operator": true,
    "no-dynamic-delete": true,
    "prefer-readonly": true,
    "newline-per-chained-call": false,
    "typeof-compare": false,
    "prefer-while": true,
    "file-name-casing": [true, "kebab-case"],
    "ban-ts-ignore": true,
    "function-constructor": true,
    "increment-decrement": false,
    "no-default-import": false,
    "unnecessary-constructor": true,
    "comment-type": false,
    "unnecessary-bind": true,
    "no-restricted-globals": true,
    "no-tautology-expression": true,
    "static-this": true,
    "unnecessary-else": true,
    "no-null-undefined-union": true,

    "no-constant-condition": true,
    "no-control-regex": true,
    "no-duplicate-case": true,
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-extra-semi": true,
    "no-inner-declarations": true,
    "no-invalid-regexp": true,
    "no-regex-spaces": true,
    "no-unexpected-multiline": true,
    "valid-jsdoc": [true, {
      "requireReturn": false,
      "requireParamType": false,
      "requireReturnType": false
    }],
    "valid-typeof": true,
    "no-multi-spaces": true,
    "handle-callback-err": true,
    "array-bracket-spacing": [
      true,
      "never"
    ],
    "block-spacing": [
      true,
      "always"
    ],
    "brace-style": [
      true,
      "1tbs"
    ],
    "ter-indent": [true, "tab"],
    "ter-max-len": [true, 120],
    "object-curly-spacing": [
      true,
      "always"
    ],
    "ter-prefer-arrow-callback": [true],
    "ter-arrow-spacing": [true],
    "ter-arrow-parens": [true, "as-needed"],
    "ter-arrow-body-style": [true, "as-needed"],
    "space-in-parens": [true, "never"],
    "sort-imports": false,
    "ter-func-call-spacing": true,
    "ter-no-irregular-whitespace": true,
    "ter-no-sparse-arrays": true,
    "ter-no-tabs": false,
    "ter-computed-property-spacing": [true, "never"],
    "ter-no-mixed-spaces-and-tabs": true,
    "ter-padded-blocks": [true, "never"],
    "ter-newline-after-var": false,
    "ter-no-proto": true,
    "ter-no-script-url": true,
    "ter-no-self-compare": true,

    "chai-prefer-contains-to-index-of": false,
    "chai-vague-errors": false,
    "export-name": false,
    "function-name": true,
    "import-name": false,
    "insecure-random": true,
    "jquery-deferred-must-complete": true,
    "max-func-body-length": false,
    "missing-jsdoc": false,
    "missing-optional-annotation": false,
    "mocha-avoid-only": false,
    "mocha-no-side-effect-code": false,
    "mocha-unneeded-done": false,
    "no-backbone-get-set-outside-model": false,
    "no-banned-terms": true,
    "no-cookies": true,
    "no-delete-expression": true,
    "no-disable-auto-sanitization": true,
    "no-document-domain": true,
    "no-document-write": true,
    "no-duplicate-parameter-names": false,
    "no-empty-interfaces": false,
    "no-empty-line-after-opening-brace": true,
    "no-exec-script": true,
    "no-for-in": true,
    "no-function-constructor-with-string-args": false,
    "no-function-expression": true,
    "no-http-string": false,
    "no-increment-decrement": false,
    "no-inner-html": true,
    "no-jquery-raw-elements": true,
    "no-missing-visibility-modifiers": false,
    "no-multiline-string": true,
    "no-octal-literal": true,
    "no-relative-imports": false,
    "no-reserved-keywords": false,
    "no-single-line-block-comment": false,
    "no-stateless-class": false,
    "no-string-based-set-immediate": true,
    "no-string-based-set-interval": true,
    "no-string-based-set-timeout": true,
    "no-suspicious-comment": true,
    "no-typeof-undefined": true,
    "no-unexternalized-strings": false,
    "no-unnecessary-bind": false,
    "no-unnecessary-field-initialization": true,
    "no-unnecessary-local-variable": true,
    "no-unnecessary-override": true,
    "no-unnecessary-semicolons": true,
    "no-unsupported-browser-code": false,
    "no-var-self": false,
    "no-with-statement": true,
    "possible-timing-attack": true,
    "prefer-array-literal": true,
    "prefer-type-cast": true,
    "promise-must-complete": true,
    "underscore-consistent-invocation": false,
    "use-named-parameter": true,
    "non-literal-require": true,
    "no-useless-files": true,
    "no-multiple-var-decl": false,
    "react-a11y-anchors": false,
    "react-a11y-aria-unsupported-elements": false,
    "react-a11y-event-has-role": false,
    "react-a11y-image-button-has-alt": false,
    "react-a11y-img-has-alt": false,
    "react-a11y-lang": false,
    "react-a11y-meta": false,
    "react-a11y-props": false,
    "react-a11y-proptypes": false,
    "react-a11y-role": false,
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-role-supports-aria-props": false,
    "react-a11y-tabindex-no-positive": false,
    "react-a11y-titles": false,
    "react-anchor-blank-noopener": false,
    "react-iframe-missing-sandbox": false,
    "react-no-dangerous-html": false,
    "react-this-binding-issue": false,
    "react-tsx-curly-spacing": false,
    "react-unused-props-and-state": false,
    "informative-docs": false,
    "non-literal-fs-path": false,
    "react-a11y-input-elements": false,
    "react-a11y-no-onchange": false,
    "react-a11y-required": false,
    "use-simple-attributes": false,
    "react-a11y-accessible-headings": false,
    "react-a11y-iframes": false,
    "void-zero": true,

    "naming-convention": false,
    "no-as-type-assertion": true,
    "no-collapsible-if": true,
    "no-else-after-return": true,
    "no-return-undefined": false,
    "no-static-this": true,
    "no-unnecessary-else": true,
    "no-var-before-return": true,
    "object-shorthand-properties-first": false,
    "parameter-properties": [
      true,
      "all-or-none",
      "leading"
    ],
    "prefer-const-enum": true,
    "oddness-check": true,
    "ext-curly": false,
    "no-unused": [true, "unused-function-expression-name", "unused-class-expression-name", "unused-catch-binding"],
    "early-exit": true,
    "const-parameters": true,
    "no-unnecessary-type-annotation": true,
    "no-accessor-recursion": true,

    "no-accessor-field-mismatch": true,
    "no-all-duplicated-branches": true,
    "no-array-delete": true,
    "no-collection-size-mischeck": true,
    "no-dead-store": true,
    "no-duplicated-branches": true,
    "no-element-overwrite": true,
    "no-empty-destructuring": true,
    "no-empty-nested-blocks": true,
    "no-gratuitous-expressions": true,
    "no-identical-conditions": true,
    "no-identical-expressions": true,
    "no-identical-functions": true,
    "no-ignored-initial-value": true,
    "no-ignored-return": true,
    "no-inconsistent-return": true,
    "no-misleading-array-reverse": true,
    "no-misspelled-operator": true,
    "no-multiline-string-literals": true,
    "no-redundant-parentheses": true,
    "no-same-line-conditional": true,
    "no-self-assignment": true,
    "no-unconditional-jump": true,
    "no-unenclosed-multiline-block": true,
    "no-unthrown-error": true,
    "no-unused-array": true,
    "no-use-of-empty-return-value": true,
    "no-useless-cast": true,
    "no-useless-increment": true,
    "no-variable-usage-before-declaration": true,
    "use-type-alias": true,
    "cognitive-complexity": false,
    "mccabe-complexity": false,
    "no-return-type-any": true,
    "no-useless-intersection": true,
    "no-commented-code": true,
    "use-primitive-type": true,
    "no-case-with-or": true,
    "prefer-immediate-return": true,
    "no-duplicate-string": false,
    "no-hardcoded-credentials": false,
    "no-small-switch": true,
    "no-redundant-boolean": true,
    "no-extra-semicolon": true,
    "no-nested-incdec": true,
    "no-big-function": false,
    "no-statements-same-line": true,
    "parameters-max-number": false,
    "consecutive-overloads": true,
    "max-union-size": false,
    "no-duplicate-in-composite": true,
    "no-in-misuse": true,
    "no-nested-template-literals": true,
    "no-redundant-jump": true,
    "no-undefined-argument": true,
    "prefer-default-last": true,
    "prefer-promise-shorthand": true,
    "no-invalid-await": true,
    "no-invariant-return": true,
    "no-inverted-boolean-check": true,
    "prefer-optional": true,
    "no-empty-array": true,
    "no-try-promise": true,
    "arguments-order": true,
    "bool-param-default": false,
    "max-switch-cases": true,
    "no-alphabetical-sort": true,
    "no-nested-switch": true,
    "no-useless-catch": true,
    "prefer-type-guard": true
  }
}