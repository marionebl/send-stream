{
  "name": "send-stream",
  "description": "Streaming file server with Range and conditional-GET support from file system or any streaming sources.",
  "repository": "github:nicolashenry/send-stream",
  "version": "0.1.0",
  "keywords": [
    "static",
    "file",
    "server",
    "send",
    "stream"
  ],
  "devDependencies": {
    "@types/content-disposition": "0.5.2",
    "@types/express": "4.17.0",
    "@types/iltorb": "2.3.0",
    "@types/koa": "2.0.48",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/mongodb": "3.1.28",
    "@types/node": "12.0.8",
    "@types/range-parser": "1.2.3",
    "@types/supertest": "2.0.7",
    "coveralls": "3.0.4",
    "cross-env": "5.2.0",
    "express": "4.17.1",
    "fastify": "2.5.0",
    "iltorb": "2.4.3",
    "koa": "2.7.0",
    "memfs": "2.15.4",
    "mocha": "6.1.4",
    "mongodb": "3.2.7",
    "nyc": "14.1.1",
    "on-finished": "2.3.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "source-map-support": "0.5.12",
    "supertest": "4.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-consistent-codestyle": "1.15.1",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.5.1",
    "typescript-tslint-plugin": "0.5.0"
  },
  "license": "MIT",
  "dependencies": {
    "content-disposition": "^0.5.3",
    "mime": "^2.4.4",
    "range-parser": "^1.2.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "clean-build": "npm run clean && npm run build",
    "watch": "tsc -w",
    "prepare": "npm run clean-build",
    "prepublishOnly": "npm run test",
    "lint": "tslint --fix -p .",
    "mocha": "nyc mocha test/*.spec.ts",
    "test": "npm run build && npm run lint && npm run mocha"
  },
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "files": [
    "dist/lib/*.js",
    "dist/lib/*.js.map",
    "dist/lib/*.d.ts"
  ],
  "nyc": {
    "include": [
      "index.ts",
      "lib/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcovonly",
      "text"
    ],
    "cache": false
  },
  "engines": {
    "node": ">= 10.14.0"
  }
}
